{
    "name": "borodulin/orm",
    "description": "Fast ORM",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Borodulin",
            "email": "borodulin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "ext-pdo": "*",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^1.1",
        "php-ds/php-ds": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpstan/phpstan": "^0.11.15",
        "php-coveralls/php-coveralls": "^2.1",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpcov": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Borodulin\\ORM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Borodulin\\ORM\\Tests\\": "tests/"
        }
    }
}
